{
  "private": true,
  "name": "try-preact",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && history-server -p 8080 ./build",
    "dev": "preact watch",
    "lint": "eslint src",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-synacor": "^2.0.2",
    "gh-pages": "^1.1.0",
    "if-env": "^1.0.0",
    "preact-cli": "^2.0.1",
    "webpack-notifier": "^1.5.0"
  },
  "dependencies": {
    "history-server": "^1.2.0",
    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "preact-router": "^2.5.7"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ]
}
